{
  "name": "psf-ecash-wallet",
  "description": "A command line (CLI) wallet app for working with eCash and eTokens.",
  "version": "v1.0.0",
  "author": "Chris Troutner <chris.troutner@gmail.com>",
  "bin": {
    "bin-cli-boilerplate": "./bin/run"
  },
  "bugs": "https://github.com/Permissionless-Software-Foundation/psf-ecash-wallet/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.3",
    "@psf/bch-js": "6.4.5",
    "axios": "0.27.2",
    "bch-encrypt-lib": "2.0.0",
    "bch-message-lib": "2.2.1",
    "bch-token-sweep": "2.1.2",
    "cli-table": "0.3.6",
    "collect.js": "4.29.0",
    "conf-cli": "0.1.9",
    "eccrypto-js": "5.4.0",
    "ipfs-coord": "7.1.6",
    "jsonrpc-lite": "2.2.0",
    "koa": "2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-router": "10.1.1",
    "minimal-ecash-wallet": "1.1.0",
    "p2wdb": "2.2.7",
    "semver": "7.3.5",
    "shelljs": "0.8.4",
    "slp-mutable-data": "2.2.7",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "chai": "4.3.4",
    "husky": "7.0.1",
    "lodash.clonedeep": "4.5.0",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "semantic-release": "19.0.5",
    "sinon": "14.0.0",
    "standard": "17.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/Permissionless-Software-Foundation/psf-ecash-wallet",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "psf-ecash-wallet",
    "plugins": [
      "@oclif/plugin-help",
      "conf-cli"
    ]
  },
  "repository": "Permissionless-Software-Foundation/psf-ecash-wallet",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --exit --timeout 15000 --recursive test/unit/",
    "test:integration": "mocha --exit --timeout 30000 --recursive test/integration/",
    "version": "oclif-dev readme && git add README.md",
    "lint": "standard --env mocha --fix",
    "coverage:report": "nyc --reporter=html mocha --exit --timeout 15000 --recursive test/unit/"
  },
  "release": {
    "publish": [
      {
        "path": "@semantic-release/npm",
        "npmPublish": false
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
